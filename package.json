{
  "name": "eslint-plugin-module-bindings-newline",
  "version": "1.0.1",
  "description": "An ESLint plugin to enforce newlines between module bindings.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "author": {
    "name": "Robert Kajic",
    "email": "robert@kajic.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/estree": "^1.0.6",
    "@typescript-eslint/utils": "^8.9.0",
    "eslint": "^9.12.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kajic/eslint-plugin-module-bindings-newline.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "module-bindings",
    "newline",
    "eslint-rules"
  ]
}
